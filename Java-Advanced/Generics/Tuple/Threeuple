package Generics_Exercises.Tuple;

public class Threeuple <F,S,T> {
    private F first;
    private S second;
    private T third;

    public Threeuple(F firstElement, S secondElement, T thirdElement) {
        this.first = firstElement;
        this.second = secondElement;
        this.third = thirdElement;
    }

    public void setFirst(F first) {
        this.first = first;
    }

    public F getFirst() {
        return this.first;
    }

    public void setSecond(S second) {
        this.second = second;
    }

    public S getSecond() {
        return this.second;
    }

    public void setThird(T third) {
        this.third = third;
    }

    public T getThird() {
        return this.third;
    }

    @Override
    public String toString() {
        return String.format("%s -> %s -> %s",this.first,this.second,this.third);
    }
}
