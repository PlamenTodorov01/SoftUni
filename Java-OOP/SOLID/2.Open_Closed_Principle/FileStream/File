package solid_lab.Open_Closed_Principle_02.p01_FileStream;

public class File implements Sendable {
    private String name;

    private int length;


    public String getName() {
        return this.name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getLength() {
        return this.length;
    }

    public void setLength(int length) {
        this.length = length;
    }

}
